{
  "name": "webpack-gas-vue-typescript-starter",
  "version": "0.1.0",
  "description": "Starter template for a TypeScript Google Apps Script project with a Vue frontend.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config ./client/webpack.prod.ts && cd build && rm client.bundle.js",
    "build:server": "webpack --config ./server/webpack.config.ts",
    "deploy": "clasp push",
    "dev": "webpack-dev-server --config ./client/webpack.dev.ts --open",
    "test": "tbd"
  },
  "author": "itho",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@google/clasp": "^2.1.0",
    "@types/google-apps-script": "0.0.45",
    "@types/node": "^12.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "gas-webpack-plugin": "^1.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
